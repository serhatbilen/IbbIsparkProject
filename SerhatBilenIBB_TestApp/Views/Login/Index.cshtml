<body>
    


    <section class="ftco-section">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-6 text-center mb-5">
                    <h2 class="heading-section">Hoşgeldiniz</h2>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-12 col-lg-10">
                    <div class="wrap d-md-flex">
                        <div class="img">
                            <img src="~/Images/İspark_logo.png" alt="Alternate Text" />
                        </div>
                        <div class="login-wrap p-4 p-md-5">
                            <div class="d-flex">
                                <div class="w-100">
                                    <h3 class="mb-4">Giriş Yap</h3>
                                </div>
                            </div>
                            @using (Html.BeginForm())
                            {
                                <div class="form-group mb-3">
                                    @Html.LabelFor(x => x.KullaniciAdi, new { @class = "label" })
                                    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control", placeholder = "Kullanıcı Adı" })
                                    @Html.ValidationMessageFor(x => x.KullaniciAdi)
                                </div>
                                <div class="form-group mb-3">
                                    @Html.LabelFor(x => x.Parola, new { @class = "label" })
                                    @Html.PasswordFor(x => x.Parola, new { @class = "form-control", placeholder = "Parola" })@Html.ValidationMessageFor(x => x.Parola)
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="form-control btn btn-primary rounded submit px-3">Giriş Yap</button>
                                </div>
                                @Html.Raw(ViewBag.Message)
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @Scripts.Render("~/scripts/jquery")
    @Scripts.Render("~/scripts/popper")
    @*@Scripts.Render("~/scripts/bootstrap")*@
    @Scripts.Render("~/scripts/main")
    @Scripts.Render("~/script/Validations")
